def sum_eo(n,t):


    if t == "e":
        start = 2
    elif t == "o":
        start = 1
    else:
        return -1
    return sum(range(start,n,2))

#x= sum_eo(11,'e')


def suma(n,t):
    suma=0
    sum_odd = 0
    for num in range(0,n):
        if num % 2 ==0:
            suma += num
        elif num % 2 !=0:
            sum_odd += num
    if t == 'e':
        return suma
    elif t == 'o' :
        return sum_odd
